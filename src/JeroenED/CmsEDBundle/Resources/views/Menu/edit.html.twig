{% extends 'JeroenEDCmsEDBundle::base.html.twig' %}
{% block body %}
{{ form_start(form) }}
{% if errors is iterable %}
    <ul>
        {% for error in errors %}
            <li>{{ error }}</li>
        {% endfor %}
    </ul>
{% endif %}

<p>{{ form_label(form.label) }}{{form_widget(form.label)}}</p>
<p>{{ form_label(form.destination) }}{{form_widget(form.destination)}}</p>
<p>{{ form_label(form.rank) }}{{form_widget(form.rank)}}</p>
<p>{{ form_widget(form.register)}}</p>

{{ form_end(form) }}
{% endblock %}