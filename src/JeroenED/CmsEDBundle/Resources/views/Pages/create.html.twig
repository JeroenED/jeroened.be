{# empty Twig template #}
{{ form_start(form) }}
{% if errors is iterable %}
    <ul>
        {% for error in errors %}
            <li>{{ error }}</li>
        {% endfor %}
    </ul>
{% endif %}

<p>{{ form_label(form.title) }}{{form_widget(form.title)}}</p>
<p>{{ form_label(form.html) }}{{form_widget(form.html)}}</p>
<p>{{ form_label(form.slug) }}{{form_widget(form.slug)}}</p>
<p>{{ form_widget(form.register)}}</p>

{{ form_end(form) }}
